    "autoprefixer": "^10.4.16",
    "eslint": "9.38.0",
    "eslint-plugin-svelte": "3.12.5",
    "postcss": "^8.4.31",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "5.43.0",
    "svelte-check": "4.3.3",
    "tailwindcss": "4.1.16",
    "typescript": "^5.1.0",
